    "#Fn = Fn-1 + Fn -2\n",
    "\n",
    "liste = [0,1]\n",
    "num = int(input(\"Please enter a number for length of fibonacci series:\\t\"))\n",
    "\n",
    "for i in range(2,num):\n",
    "    i = liste[i-1] + liste[i-2]\n",
    "    liste.append(i)\n",
    "print(f\"Fibonacci series is : {liste}\")\n",
